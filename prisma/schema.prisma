datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Account {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  email         String   @unique
  emailVerified Boolean  @default(false)
  password      String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}